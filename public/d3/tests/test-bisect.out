exact match:
    -1 -> 0 0
     0 -> 0 0
     1 -> 0 1
     2 -> 1 2
     3 -> 2 3
     4 -> 3 4
     5 -> 4 4

non-exact match:
   -.1 -> 0 0
    .1 -> 0 0
   1.1 -> 1 1
   2.1 -> 2 2
   3.1 -> 3 3
   4.1 -> 4 4
   5.1 -> 4 4

weird values:
        NaN -> 0 4
   Infinity -> 4 4
  -Infinity -> 0 0

d3.bisect === d3.bisectRight:
   true

lo:
    -1 -> 2 2
     0 -> 2 2
     1 -> 2 2
     2 -> 2 2
     3 -> 2 3
     4 -> 3 4
     5 -> 4 4

hi:
    -1 -> 0 0
     0 -> 0 0
     1 -> 0 1
     2 -> 1 2
     3 -> 2 2
     4 -> 2 2
     5 -> 2 2

lo and hi:
    -1 -> 1 1
     0 -> 1 1
     1 -> 1 1
     2 -> 1 2
     3 -> 2 3
     4 -> 3 3
     5 -> 3 3

